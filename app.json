      "package": "com.mumartoor.Meditation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "eb300846-3f97-4560-91be-a219016caab8"
      }
    },
    "plugins": ["expo-video"]
